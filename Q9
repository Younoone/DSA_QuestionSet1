public class Solution {
    public static int[] generateFibonacciNumbers(int n) {
        // Write your code here.
        int[] arr=new int[n];
        int a=0;
        int b=1;
        for(int i=0;i<n;i++){
            arr[i]=a;
            int x = a;
            a=b;
            b=b+x;
        }
        return arr;
    }
}
