public class Solution {
    public static int[] reverseArray(int n, int []nums) {
        // Write your code here.
        int i = 0;
        int j = n-1;
        while(i<=j){
            int a=nums[i];
            nums[i]=nums[j];
            nums[j] = a;
            i++;
            j--;
        }
        return nums;
    }
}
